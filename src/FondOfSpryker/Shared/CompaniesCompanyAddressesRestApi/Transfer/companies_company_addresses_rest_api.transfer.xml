<?xml version="1.0"?>
<transfers xmlns="spryker:transfer-01"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="spryker:transfer-01 http://static.spryker.com/transfer-01.xsd">

    <transfer name="RestCompanyUnitAddressAttributes">
        <property name="uuid" type="string"/>
        <property name="name1" type="string"/>
        <property name="name2" type="string"/>
        <property name="address1" type="string"/>
        <property name="address2" type="string"/>
        <property name="address3" type="string"/>
        <property name="city" type="string"/>
        <property name="zipCode" type="string"/>
        <property name="country" type="string"/>
        <property name="phone" type="string"/>
        <property name="fax" type="string"/>
        <property name="comment" type="string"/>
        <property name="gln" type="string"/>
        <property name="isDefaultBilling" type="bool"/>
        <property name="isDefaultShipping" type="bool"/>
        <property name="isDeleted" type="bool"/>
    </transfer>

    <transfer name="RestCompaniesCompanyAddressesDeleteRequest">
        <property name="idCustomer" type="int"/>
        <property name="companyUuid" type="string"/>
        <property name="companyUnitAddressUuid" type="string"/>
    </transfer>

    <transfer name="RestCompaniesCompanyAddressesDeleteResponse">
        <property name="isSuccessful" type="bool"/>
        <property name="messages" type="Message[]"  singular="message"/>
    </transfer>

    <transfer name="Message">
        <property name="value" type="string"/>
    </transfer>
</transfers>
